# SPARKA Frontend Environment Configuration (Development)

# Session Configuration
SESSION_SECRET=samakan dengan backend

# API Configuration
# Base API URL without /api suffix - will be automatically added by apiConfig
API_URL=http://localhost:8000 (backend)
NEXT_PUBLIC_API_URL=http://localhost:8000 (backend)
NEXT_PUBLIC_AI_API_URL=http://localhost:5002 (container integration for AI services)

# Streaming Configuration
NEXT_PUBLIC_STREAMING_URL=http://localhost:8010 (container streaming service)
NEXT_PUBLIC_STREAMING_INTERNAL_URL=http://localhost:8010 (container streaming service internal)

# Application Configuration
NEXT_PUBLIC_APP_NAME=SPARKA
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_DESCRIPTION="Smart Parking Management System"

# Environment
NODE_ENV=development
NEXT_PUBLIC_ENV=development

# Base URL
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# WebSocket Configuration
NEXT_PUBLIC_WS_URL=ws://localhost:8010 (container streaming service)
